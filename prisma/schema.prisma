generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum Status {
  high
  low
  fixed
}

model Coin {
  id         Int      @id @default(autoincrement())
  name       String
  price      String
  change     String
  lowest     String
  highest    String
  time       String
  status     Status
  updateTime DateTime @default(now())
}
